{
	"extends": ["tslint:latest", "tslint-config-prettier"],
	"linterOptions": {
		"exclude": ["config/**/*.js", "node_modules/**/*.ts"]
	},
	"rules": {
		"no-console": false,
		"member-access": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"interface-name": false,
		"no-submodule-imports": false,
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore",
			"allow-pascal-case"
		]
	}
}
